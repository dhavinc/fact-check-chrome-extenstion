{
  "name": "INTOX BUSTER",
  "version": "1.0",
  "description": "Bust all INTOX in tour feed",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "https://www.facebook.com/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/intox-buster.png",
      "32": "images/intox-buster.png",
      "48": "images/intox-buster.png",
      "128": "images/intox-buster.png"
    }
  },
  "icons": {
    "16": "images/intox-buster.png",
    "32": "images/intox-buster.png",
    "48": "images/intox-buster.png",
    "128": "images/intox-buster.png"
  },
  "web_accessible_resources": ["fonts/*"],
  "manifest_version": 2
}
